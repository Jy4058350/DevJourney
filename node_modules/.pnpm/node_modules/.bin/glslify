#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shanbianchun/Documents/0802/DevJourney/node_modules/.pnpm/glslify@7.1.1/node_modules/glslify/node_modules:/Users/shanbianchun/Documents/0802/DevJourney/node_modules/.pnpm/glslify@7.1.1/node_modules:/Users/shanbianchun/Documents/0802/DevJourney/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shanbianchun/Documents/0802/DevJourney/node_modules/.pnpm/glslify@7.1.1/node_modules/glslify/node_modules:/Users/shanbianchun/Documents/0802/DevJourney/node_modules/.pnpm/glslify@7.1.1/node_modules:/Users/shanbianchun/Documents/0802/DevJourney/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../glslify/bin.js" "$@"
else
  exec node  "$basedir/../glslify/bin.js" "$@"
fi
